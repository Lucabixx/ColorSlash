  flutter:
    sdk: flutter
  firebase_auth: ^6.0.2
  firebase_core: ^4.1.0
  firebase_messaging: ^16.0.1
  firebase_storage: ^13.0.1
  cloud_firestore: ^6.0.1
  google_sign_in: ^7.1.1
  google_sign_in_android: ^7.0.5
  google_sign_in_ios: ^6.1.0
  google_sign_in_platform_interface: ^3.1.0
  google_sign_in_web: ^1.0.0
  sqflite: ^2.3.0
  path_provider: ^2.1.1
  permission_handler: ^12.0.1
  permission_handler_android: ^13.0.1
  image_picker: ^1.0.4
  video_player: ^2.7.2
  file_picker: ^10.3.3
  speech_to_text: ^7.2.0
  characters: ^1.4.0
  material_color_utilities: ^0.11.1
  meta: ^1.16.0
  flutter_local_notifications: ^19.4.2
  flutter_local_notifications_linux: ^6.0.0
  flutter_local_notifications_platform_interface: ^9.1.0
  flutter_sound_platform_interface: ^9.28.0
  flutter_sound: ^9.28.0
  flutter_sound_web: ^9.28.0 
  provider: ^6.1.1
  flutter_staggered_grid_view: ^0.7.0
  animations: ^2.0.8
  cupertino_icons: ^1.0.8
  flutter_native_splash: ^2.3.7
  flutter_launcher_icons: ^0.14.4
  test_api: ^0.7.6
  timezone: ^0.10.1
  logger: ^2.0.2
  uuid: any
  path: any

dev_dependencies:
  flutter_test:
    sdk: flutter

